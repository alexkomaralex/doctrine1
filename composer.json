{
    "name": "alexkomaralex/doctrine1",
    "type": "library",
    "description": "PHP 5 Doctrine1 Object Relational Mapper (ORM)",
    "keywords": [
        "php",
        "doctrine1",
        "orm",
        "database"
    ],
    "homepage": "http://www.doctrine-project.org",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {"name": "Konsta Vesterinen", "email": "kvesteri@cc.hut.fi"},
        {"name": "Jonathan Wage", "email": "jonwage@gmail.com"}
    ],
    "replace": {
        "doctrine/doctrine1": "*"
    },
    "require": {
        "php": ">=5.3.3",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-0": {
            "Doctrine_": "lib/"
        },
        "classmap": [
            "lib/Doctrine.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "archive": {
        "exclude": ["/docs", "/tests", "/tools"]
    },
    "scripts": {
        "test": "@php ./tests/run.php"
    }
}
